<h1>issue page</h1>
<%=projectDet %>

<div class="reportForm">
  <form action="/isseu" method="post" id="reportissueform">
    <input type="text" name="title" placeholder="issue title" />
    <input type="description" name="desc" placeholder="issue description" />
    <input type="description" name="author" placeholder="Author Name" />
    <input type="hidden" name="projectRef" value="<%= projectid %>" />
    <select name="lable" id="selectBox" onchange="selectlabel()" required>
      <option value="" disabled selected>Select issue lable</option>
      <option>Personal</option>
      <option>Work</option>
      <option>Claeaning</option>
      <option>Otheres</option>
    </select>
    <div id="valueSelected"></div>
    <input type="submit" value="submit" />
  </form>
</div>
